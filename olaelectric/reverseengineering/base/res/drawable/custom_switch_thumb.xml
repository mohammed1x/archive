<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:dither="false" android:visible="true" android:shape="rectangle" android:useLevel="false">
            <corners android:radius="100.0dip" />
            <solid android:color="@color/white_FFFFFF" />
            <size android:height="24.0dip" android:width="24.0dip" />
            <stroke android:width="6.0dip" android:color="@color/transparent" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:dither="true" android:visible="true" android:shape="rectangle" android:useLevel="false">
            <corners android:radius="100.0dip" />
            <solid android:color="@color/white" />
            <size android:height="24.0dip" android:width="24.0dip" />
            <stroke android:width="6.0dip" android:color="@color/transparent" />
        </shape>
    </item>
</selector>
